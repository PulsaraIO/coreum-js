    "@cosmjs/stargate": "^0.30.1",
    "@cosmjs/tendermint-rpc": "^0.30.1",
    "@cosmos-kit/core": "^1.3.4",
    "@cosmos-kit/cosmostation-extension": "^0.15.16",
    "@cosmos-kit/keplr-extension": "^0.33.16",
    "@cosmos-kit/leap-extension": "^0.14.16",
    "@cosmos-kit/react-lite": "^1.3.15",
    "@walletconnect/client": "^1.8.0",
    "bech32": "^2.0.0",
