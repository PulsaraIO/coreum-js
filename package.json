{
  "name": "coreum-js",
  "version": "2.6.1",
  "description": "JS/TS Library to to easily make use of the Coreum Blockchain",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "scripts": {
    "build": "rm -rf ./dist && npm run build:main && npm run build:module",
    "test": "ts-node test.ts",
    "build:version": "npm run build && standard-version && git add -A",
    "build:main": "npx tsc -p tsconfig.json",
    "build:module": "npx tsc -p tsconfig.module.json",
    "generate:docs": "npx typedoc --plugin typedoc-plugin-missing-exports src/index.ts"
  },
  "pre-commit": [
    "build:version"
  ],
  "standard-version": {
    "skip": {
      "commit": true
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@cosmjs/amino": "^0.30.1",
    "@cosmjs/cosmwasm-stargate": "^0.30.1",
    "@cosmjs/crypto": "^0.30.1",
    "@cosmjs/math": "^0.30.1",
    "@cosmjs/proto-signing": "^0.30.1",
    "@cosmjs/stargate": "^0.30.1",
    "@cosmjs/tendermint-rpc": "^0.30.1",
    "@cosmostation/cosmos-client": "^0.0.5",
    "@cosmostation/extension-client": "^0.1.15",
    "bech32": "^2.0.0",
    "bignumber.js": "^9.1.1",
    "dcent-web-connector": "^0.12.0",
    "eventemitter3": "^5.0.0",
    "pre-commit": "^1.2.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@types/node": "^18.15.9",
    "@types/react": "^18.0.37",
    "@types/ws": "^8.5.4",
    "cosmjs-types": "^0.7.1",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.4",
    "typedoc-plugin-missing-exports": "^2.0.0",
    "typescript": "^5.0.2"
  }
}
